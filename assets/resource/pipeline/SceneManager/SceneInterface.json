{
    "SceneEnterMapDijiang": {
        "doc": "进入帝江号地图界面",
        "recognition": {
            "type": "And",
            "param": {
                "all_of": [
                    {
                        "recognition": {
                            "type": "TemplateMatch",
                            "param": {
                                "roi": [
                                    -350,
                                    0,
                                    350,
                                    60
                                ],
                                "template": [
                                    "SceneManager/MapMind.png"
                                ]
                            }
                        }
                    },
                    {
                        "recognition": {
                            "type": "OCR",
                            "param": {
                                "roi": [
                                    0,
                                    0,
                                    350,
                                    220
                                ],
                                "expected": [
                                    "[Oo].?[Mm].?[Vv]"
                                ]
                            }
                        }
                    }
                ]
            }
        },
        "inverse": true,
        "next": [
            "[JumpBack]ScenePrivateAnyEnterWorld",
            "ScenePrivateWorldEnterMapDijiang"
        ]
    },
